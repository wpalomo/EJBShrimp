package anexos.TO;

import anexos.sri.ws.autorizacion.Autorizacion;
import anexos.sri.ws.autorizacion.RespuestaComprobante;

/**
 *
 * @author Carlos
 */
public class AnxComprobanteAutorizadoTO implements java.io.Serializable{
    private String claveAcceso;
    private String tipo;
    private String ambiente;
    private String rasonSocialEmisor;
    private String rucEmisor;
    private String estado;
    private String numeroDocumento;
    private String fechaEmision;
    private String numeroAutorizacion;
    private String fechaAutorizacion;
    private String total;
    private anexos.sri.ws.autorizacion.Autorizacion comprobante;
    private byte[] eXml;

    public AnxComprobanteAutorizadoTO() {
    }

    public AnxComprobanteAutorizadoTO(String claveAcceso, String tipo, String ambiente, String rasonSocialEmisor, String rucEmisor, String estado, String numeroDocumento, String fechaEmision, String numeroAutorizacion, String fechaAutorizacion, String total, Autorizacion comprobante, byte[] eXml) {
        this.claveAcceso = claveAcceso;
        this.tipo = tipo;
        this.ambiente = ambiente;
        this.rasonSocialEmisor = rasonSocialEmisor;
        this.rucEmisor = rucEmisor;
        this.estado = estado;
        this.numeroDocumento = numeroDocumento;
        this.fechaEmision = fechaEmision;
        this.numeroAutorizacion = numeroAutorizacion;
        this.fechaAutorizacion = fechaAutorizacion;
        this.total = total;
        this.comprobante = comprobante;
        this.eXml = eXml;
    }
   
    public String getAmbiente() {
        return ambiente;
    }

    public void setAmbiente(String ambiente) {
        this.ambiente = ambiente;
    }

    public String getClaveAcceso() {
        return claveAcceso;
    }

    public void setClaveAcceso(String claveAcceso) {
        this.claveAcceso = claveAcceso;
    }

    public Autorizacion getComprobante() {
        return comprobante;
    }

    public void setComprobante(Autorizacion comprobante) {
        this.comprobante = comprobante;
    }

  

    public byte[] geteXml() {
        return eXml;
    }

    public void seteXml(byte[] eXml) {
        this.eXml = eXml;
    }

    public String getEstado() {
        return estado;
    }

    public void setEstado(String estado) {
        this.estado = estado;
    }

    public String getFechaAutorizacion() {
        return fechaAutorizacion;
    }

    public void setFechaAutorizacion(String fechaAutorizacion) {
        this.fechaAutorizacion = fechaAutorizacion;
    }

    public String getFechaEmision() {
        return fechaEmision;
    }

    public void setFechaEmision(String fechaEmision) {
        this.fechaEmision = fechaEmision;
    }

    public String getNumeroAutorizacion() {
        return numeroAutorizacion;
    }

    public void setNumeroAutorizacion(String numeroAutorizacion) {
        this.numeroAutorizacion = numeroAutorizacion;
    }

    public String getNumeroDocumento() {
        return numeroDocumento;
    }

    public void setNumeroDocumento(String numeroDocumento) {
        this.numeroDocumento = numeroDocumento;
    }

    public String getRasonSocialEmisor() {
        return rasonSocialEmisor;
    }

    public void setRasonSocialEmisor(String rasonSocialEmisor) {
        this.rasonSocialEmisor = rasonSocialEmisor;
    }

    public String getRucEmisor() {
        return rucEmisor;
    }

    public void setRucEmisor(String rucEmisor) {
        this.rucEmisor = rucEmisor;
    }

    public String getTipo() {
        return tipo;
    }

    public void setTipo(String tipo) {
        this.tipo = tipo;
    }

    public String getTotal() {
        return total;
    }

    public void setTotal(String total) {
        this.total = total;
    }
}
