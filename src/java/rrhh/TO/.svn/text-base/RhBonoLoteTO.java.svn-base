package rrhh.TO;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 *
 * @author misael
 */
public class RhBonoLoteTO implements Serializable{
    private String empId;
    private String empApellidosNombres;
    private java.math.BigDecimal empValor;
    private boolean empDeducible;

    public RhBonoLoteTO() {
    }

    public RhBonoLoteTO(String empId, String empApellidosNombres, BigDecimal empValor, boolean empDeducible) {
        this.empId = empId;
        this.empApellidosNombres = empApellidosNombres;
        this.empValor = empValor;
        this.empDeducible = empDeducible;
    }

    public String getEmpApellidosNombres() {
        return empApellidosNombres;
    }

    public void setEmpApellidosNombres(String empApellidosNombres) {
        this.empApellidosNombres = empApellidosNombres;
    }

    public boolean isEmpDeducible() {
        return empDeducible;
    }

    public void setEmpDeducible(boolean empDeducible) {
        this.empDeducible = empDeducible;
    }

    public String getEmpId() {
        return empId;
    }

    public void setEmpId(String empId) {
        this.empId = empId;
    }

    public BigDecimal getEmpValor() {
        return empValor;
    }

    public void setEmpValor(BigDecimal empValor) {
        this.empValor = empValor;
    }

}
